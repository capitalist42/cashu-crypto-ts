{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2020",
		"lib": ["ES2020", "DOM"],
		"declaration": true,
		"outDir": "./modules/esm",
		"moduleResolution": "Node16",
		"noImplicitAny": true,
		"sourceMap": true,
		"esModuleInterop": false,
		"strict": true
	},
	"include": ["src/**/*"]
}
